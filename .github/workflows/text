      - name: 调试输出环境变量
        run: |
          echo "url 是否设置: $([ -n \"${{ secrets.url }}\" ] && echo '是' || echo '否')"
          echo "url 长度: ${#{{ secrets.url }}}"
          echo "filename 是否设置: $([ -n \"${{ secrets.filename }}\" ] && echo '是' || echo '否')"
          echo "filename: ${{ secrets.filename }}"
          echo "git_user 是否设置: $([ -n \"${{ secrets.git_user }}\" ] && echo '是' || echo '否')"
          echo "git_email 是否设置: $([ -n \"${{ secrets.git_email }}\" ] && echo '是' || echo '否')"
